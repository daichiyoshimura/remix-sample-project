{
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"useTabs": true,
	"trailingComma": "all",
	"printWidth": 100,
	"importOrder": [
		"^@hooks/(.*)$",
		"^@apis/(.*)$",
		"^@components/(.*)$",
		"^@features/(.*)$",
		"^@loaders/(.*)$",
		"^@actions/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true
}
